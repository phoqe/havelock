{
  "name": "havelock",
  "version": "2.4.0",
  "description": "Extract accounts, cookies, and history from Chromium-based web browsers.",
  "keywords": [
    "chromium",
    "chrome",
    "google",
    "web",
    "browser",
    "password",
    "cookies",
    "history"
  ],
  "bugs": {
    "url": "https://github.com/phoqe/havelock/issues",
    "email": "phoqe@phoqe.dev"
  },
  "license": "MIT",
  "main": "havelock.js",
  "author": {
    "name": "Linus LÃ¥ngberg",
    "email": "phoqe@phoqe.dev",
    "url": "https://phoqe.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phoqe/havelock.git"
  },
  "private": false,
  "dependencies": {
    "commander": "^7.2.0",
    "keytar": "^7.2.0",
    "sqlite3": "^5.0.0",
    "prettier": "2.2.1"
  },
  "scripts": {
    "format": "prettier --write --ignore-unknown ."
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.2.1",
    "lint-staged": "^11.0.0",
    "semantic-release": "^17.4.3",
    "@semantic-release/git": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bin": {
    "havelock": "./cli/cli.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failComment": false,
          "failTitle": false,
          "labels": false,
          "releasedLabels": false
        }
      ]
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
